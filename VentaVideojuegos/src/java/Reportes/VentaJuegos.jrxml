	</parameter>
	<queryString>
		<![CDATA[SELECT
     usuario.`Nombre` AS usuario_Nombre,
     producto.`nombre` AS producto_nombre,
     producto.`precio` AS producto_precio,
     reservar.`Fecha_Reserva` AS reservar_Fecha_Reserva,
     reservar.`Cantidad_Articulos` AS reservar_Cantidad_Articulos,
     reservar.`preciototal` AS reservar_preciototal,
     reservar.`Codigo_Reserva` AS reservar_Codigo_Reserva,
     usuario.`IDUsuario` AS usuario_IDUsuario
FROM
     `producto` producto INNER JOIN `reservar` reservar ON producto.`id_producto` = reservar.`id_producto`
     INNER JOIN `usuario` usuario ON reservar.`IDUsuario` = usuario.`IDUsuario`
WHERE
     usuario.IDUsuario = $P!{id_usuario}]]>
	</queryString>
